{
  "name": "test.service",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "start": "node -r ts-node/register/transpile-only -r tsconfig-paths/register ./dist/src/index.js",
    "build": "tsc -p tsconfig.prod.json",
    "pm2": "env NODE_ENV=production pm2 start ./dist/index.js -n \"videoworkers-api\" --node-args=\"-r ts-node/register/transpile-only -r tsconfig-paths/register\"",
    "test": "jest --verbose",
    "sync-db": "ts-node ./src/database/models/index.js",
    "dev": "nodemon",
    "db:create": "pnpm sequelize db:create",
    "db:drop": "pnpm sequelize db:drop",
    "db:migrate": "pnpm sequelize db:migrate --config src/database/system/config/config.js --migrations-path src/database/migrations",
    "db:reset": "pnpm db:drop & pnpm db:create & pnpm db:migrate",
    "db:create:test": "env NODE_ENV=test pnpm db:create",
    "db:migrate:test": "env NODE_ENV=test pnpm run db:migrate",
    "db:seed": "ts-node ./node_modules/.bin/sequelize db:seed:all --seeders-path src/database/seeders",
    "db:drop:test": "env NODE_ENV=test pnpm db:drop",
    "db:reset:test": "env NODE_ENV=test ts-node ./node_modules/.bin/sequelize db:drop & ts-node ./node_modules/.bin/sequelize db:create & ts-node ./node_modules/.bin/sequelize db:migrate",
    "lint": "eslint src/",
    "lint:fix": "eslint --fix src/",
    "format": "prettier --write **/*.ts",
    "format:check": "prettier --check **/*.ts"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "axios": "^1.7.7",
    "cors": "^2.8.5",
    "dotenv": "^16.4.5",
    "express": "^4.21.1",
    "joi": "^17.13.3",
    "jsonwebtoken": "^9.0.2",
    "moment": "^2.30.1",
    "morgan": "^1.10.0",
    "pg": "^8.13.1",
    "pg-hstore": "^2.3.4",
    "sequelize": "^6.37.5",
    "sequelize-typescript": "^2.1.6",
    "slugify": "^1.6.6",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.6.3",
    "uuid": "^11.0.3",
    "winston": "^3.17.0"
  },
  "devDependencies": {
    "@types/cors": "^2.8.17",
    "@types/dotenv": "^8.2.3",
    "@types/express": "^5.0.0",
    "@types/jest": "^29.5.14",
    "@types/joi": "^17.2.3",
    "@types/jsonwebtoken": "^9.0.7",
    "@types/morgan": "^1.9.9",
    "@types/node": "^22.9.1",
    "@types/supertest": "^6.0.2",
    "jest": "^29.7.0",
    "sequelize-cli": "^6.6.2",
    "supertest": "^7.0.0",
    "ts-node-dev": "^2.0.0"
  }
}
